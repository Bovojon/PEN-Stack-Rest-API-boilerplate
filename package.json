{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "dist",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "nodemon --exec babel-node ./src/bin/www.js",
    "serve": "node dist/bin/www.js"
  },
  "babel": {
    "presets": ["@babel/preset-env"]
  },
  "dependencies": {
    "express": "~4.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
