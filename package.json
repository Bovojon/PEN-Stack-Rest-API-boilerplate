{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "dist",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "serve": "node dist/bin/www.js",
    "start": "nodemon --exec babel-node src/bin/www.js"
  },
  "babel": {
    "presets": ["@babel/preset-env"],
    "plugins": ["@babel/transform-runtime"]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}
